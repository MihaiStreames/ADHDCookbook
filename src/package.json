{
	"name": "adhd-cookbook",
	"version": "1.0.0",
	"type": "module",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "echo \"No tests specified yet\"",
		"build:android": "eas build --platform android --profile development --local",
		"build:ios": "eas build --platform ios --profile development --local",
		"prebuild": "expo prebuild",
		"eject": "expo eject",
		"clean": "rm -rf node_modules && npm install",
		"prepare-release": "echo \"Preparing v$(node -p \"require('./package.json').version\") release\"",
		"validate": "expo doctor"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "1.23.1",
		"@react-navigation/native": "^6.1.9",
		"@react-navigation/native-stack": "^6.9.17",
		"expo": "~52.0.38",
		"expo-checkbox": "~4.0.1",
		"expo-image-picker": "~16.0.6",
		"expo-status-bar": "~2.0.1",
		"react": "18.3.1",
		"react-native": "0.76.7",
		"react-native-safe-area-context": "4.12.0",
		"react-native-screens": "~4.4.0",
		"expo-dev-client": "~5.0.14"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@eslint/js": "^8.57.0",
		"@types/react": "~18.3.12",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-native": "^4.1.0",
		"globals": "^15.0.0",
		"typescript": "^5.3.3"
	},
	"private": true
}
